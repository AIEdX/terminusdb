{ "@type" : "@context",
  "@base" : "terminusdb://ref/data/",
  "@schema" : "http://terminusdb.com/schema/ref#",
  "layer" : "http://terminusdb.com/schema/layer#",
  "xsd" : "http://www.w3.org/2001/XMLSchema#" }

{ "@id" : "layer:Layer",
  "@type" : "Class",
  "@key" : { "@type" : "ValueHash" },
  "layer:identifier" : "xsd:string" }

{ "@id" : "Branch",
  "@type" : "Class",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "name" ] },
  "head" : { "@type" : "Optional",
             "@class" : "Commit" },
  "name" : "xsd:string" }

{ "@id" : "Commit",
  "@type" : "Class",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "identifier" ] },
  "instance" : { "@type" : "Optional",
                 "@class" : "Graph" },
  "schema" : { "@type" : "Optional",
               "@class" : "Graph" },
  "author" : "xsd:string",
  "message" : "xsd:string",
  "identifier" : "xsd:string",
  "timestamp" : "xsd:decimal",
  "parent" : { "@type" : "Optional",
               "@class" : "Commit" } }

{ "@id" : "Graph",
  "@type" : "Class",
  "name" : "xsd:string",
  "layer" : "layer:Layer" }

{ "@id" : "ValidCommit",
  "@type" : "Class",
  "@inherits" : "Commit",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "identifier" ] } }


{ "@id" : "InvalidCommit",
  "@type" : "Class",
  "@inherits" : "Commit",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "identifier" ] } }

{ "@id" : "Ref",
  "@type" : "Class",
  "commit" : "Commit",
  "identifier" : "xsd:string" }

{ "@id" : "Tag",
  "@type" : "Class",
  "@inherits" : "Ref" }
