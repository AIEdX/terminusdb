{ "@type" : "@context",
  "@base" : "http://terminusdb.com/db/repository/data/",
  "@schema" : "http://terminusdb.com/db/repository/schema#",
  "xsd" : "http://www.w3.org/2001/XMLSchema#"}

{ "@id" : "layer:Layer",
  "@type" : "Class",
  "@key" : { "@type" : "ValueHash" },
  "layer:identifier" : "xsd:string" }

{ "@id" : "Repository",
  "@type" : "Class",
  "head" : "layer:Layer",
  "name" : "xsd:string" }

{ "@id" : "Local",
  "@type" : "Class",
  "@inherits" : "Repository",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "name" ] } }

{ "@id" : "Remote",
  "@type" : "Class",
  "@inherits" : "Repository",
  "remote_url" : "xsd:string",
  "@key" : { "@type" : "Lexical",
             "@fields" : [ "name" ] } }
